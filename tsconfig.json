{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "lib": ["es2015", "dom"],
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "file-explorer": ["src"],
      "file-explorer/*": ["src/*", "docs/*", "*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*", "docs-src/**/*"]
}
